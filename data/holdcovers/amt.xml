<!DOCTYPE cubical-hold-covers>
<holdcovers sprite="stages/phoneCallStreet/strum-holds" alpha="1" scale="0.75" flipY="true">
	<strum id="0"> <!-- LEFT -->
		<anim name="startleft"   anim="hold splash purple 1" fps="24" x="10" y="50" loop="false" />
		<anim name="holdingleft" anim="hold splash purple 1"       fps="24" x="10" y="50" loop="true" />
		<anim name="endleft"     anim="note splash purple end 1"   fps="24" x="10" y="50" loop="false" />
	</strum>
	<strum id="1"> <!-- DOWN -->
		<anim name="startdown"   anim="hold splash blue 1" fps="24" x="10" y="50" loop="false" />
		<anim name="holdingdown" anim="hold splash blue 1"       fps="24" x="10" y="50" loop="true" />
		<anim name="enddown"     anim="note splash blue end 1"   fps="24" x="10" y="50" loop="false" />
	</strum>
	<strum id="2"> <!-- UP -->
		<anim name="startup"   anim="hold splash green 1" fps="24" x="10" y="50" loop="false" />
		<anim name="holdingup" anim="hold splash green 1"       fps="24" x="10" y="50" loop="true" />
		<anim name="endup"     anim="note splash green end 1"   fps="24" x="10" y="50" loop="false" />
	</strum>
	<strum id="3"> <!-- RIGHT -->
		<anim name="startright"   anim="hold splash red 1" fps="24" x="10" y="50" loop="false" />
		<anim name="holdingright" anim="hold splash red 1"       fps="24" x="10" y="50" loop="true" />
		<anim name="endright"     anim="note splash red end 1"   fps="24" x="10" y="50" loop="false" />
	</strum>
</holdcovers>